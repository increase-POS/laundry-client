                               Margin="5 10 15 10" FontSize="18" Text="KWD"
                            Grid.Row="1"   x:Name="tb_moneyIconTotal" />
                </Grid>
            </Border>
            <Grid Grid.Row="3">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="*"/>
                    <ColumnDefinition Width="auto"/>
                </Grid.ColumnDefinitions>
                <Border  Grid.Row="3" Grid.Column="0" Height="40" Tag="ssssssss" Background="{StaticResource MainColor}" CornerRadius="7.5"  
                     Margin="0 15 5 0"  Padding="0">
                    <Border.Effect>
                        <DropShadowEffect
                          ShadowDepth="1"
                          Direction="360"
                          Color="Gray"
                          Opacity="0.3"
                         BlurRadius="10"/>
                    </Border.Effect>
                    <Button  Content="Pay" Name="btn_pay"   FontSize="20" FontWeight="Bold"
                    materialDesign:ButtonAssist.CornerRadius="7" Height="40"
                             Padding="5 0"
                          Background="{x:Null}" BorderBrush="{x:Null}"
                       Foreground="White" Click="Btn_pay_Click"  />
                </Border>
                <Border  Name="brd_cancel" Visibility="Visible" Grid.Row="3" Grid.Column="1" Height="40" Tag="ssssssss" Background="{StaticResource Red}" CornerRadius="7.5"  
                     Margin="0 15 5 0"  Padding="0">
                    <Border.Effect>
                        <DropShadowEffect
                          ShadowDepth="1"
                          Direction="360"
                          Color="Gray"
                          Opacity="0.3"
                         BlurRadius="10"/>
                    </Border.Effect>
                    <Button Content="X" Name="btn_cancel"   FontSize="22" FontWeight="Bold"
                    materialDesign:ButtonAssist.CornerRadius="7" Height="40" Width="50"
                         Padding="5 0" 
                             Background="{x:Null}" BorderBrush="{x:Null}"
                       Foreground="White" IsEnabled="False"  Click="Btn_cancel_Click" />
                </Border>

            </Grid>
            
        </Grid>
        <!--#endregion-->
    </Grid>
</UserControl>
