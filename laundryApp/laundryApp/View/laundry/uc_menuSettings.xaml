                    <TextBlock Grid.Column="1" Text="Items" x:Name="txt_title" Margin="10,0,0,0"  Foreground="White" 
                     FontSize="14"     VerticalAlignment="Center" 
                           FontWeight="Bold" />
                    <Button   x:Name="btn_clear" Click="Btn_clear_Click" Grid.Column="3" Padding="0" 
                               VerticalAlignment="Center" Background="{x:Null}" BorderBrush="{x:Null}"   >
                            <materialDesign:PackIcon Foreground="White" Kind="RefreshCircle"   Width="30" Height="30"/>
                        </Button>

                </Grid>
            </Border>

            <ScrollViewer Background="White"  Grid.Row="1"  VerticalScrollBarVisibility="Auto" >
                <StackPanel>
                    <!--<Grid >
                        <Grid.RowDefinitions >
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="auto"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>-->
                    <DockPanel Margin="10 0" LastChildFill="True"  >
                        <TextBlock x:Name="txt_daysOfWeek" Text="Days of week" Foreground="{StaticResource SecondColor}" FontSize="12" FontWeight="Bold" VerticalAlignment="Center" Margin="5,5,5,5"/>
                        <Separator  Background="{StaticResource MainColor}" VerticalAlignment="Center" Width="auto"  />
                    </DockPanel>
                        <Grid Grid.Row="1">
                            <TextBlock  Grid.Row="1" Grid.Column="0" x:Name="txt_name"
                                    Text="item name" FontWeight="SemiBold" FontSize="14"
                                   Margin="10 5" Foreground="{StaticResource ThickGrey}"
                                   HorizontalAlignment="Left" VerticalAlignment="Center"   />
                            <TextBlock  Grid.Row="1" Grid.Column="1" Text="{Binding Path=name}"
                                   Margin="5,5,15,5" Foreground="{StaticResource ThickGrey}"
                                   HorizontalAlignment="Right"   VerticalAlignment="Center" />
                        </Grid>
                    <Grid Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="auto"/>
                        </Grid.ColumnDefinitions>
                        <Border Grid.Row="8" CornerRadius="10" Margin="10,5,5,5"   BorderThickness="1" BorderBrush="{StaticResource LightGrey}">
                        <Grid>
                            <TextBox x:Name="tb_preparingTime" Grid.Row="8"  Padding="10,0,5,0"  
                                         Text="{Binding preparingTime, Mode=OneWay}"
                                         Style="{StaticResource MaterialDesignFloatingHintTextBox}"
                                         materialDesign:HintAssist.Hint="preparing time..."    
                           Foreground="#4E4E4E"    
                                     BorderThickness="0" Tag="decimal"
                                 BorderBrush="AliceBlue" MaxLength="100" PreviewTextInput="Number_PreviewTextInput"
                    materialDesign:TextFieldAssist.CharacterCounterStyle="{Binding}"
                  Height="40"   Margin="4,0"  PreviewKeyDown="Spaces_PreviewKeyDown" />

                            <Path Fill="#E81123" x:Name="p_error_preparingTime" Stretch="Fill" Margin="5,5,15,5"
                              HorizontalAlignment="Right" Grid.Row="8"  Visibility="Collapsed"   
                         VerticalAlignment="Center" Width="15"  Height="15" 
                             Data="{StaticResource infoCircle}"/>

                        </Grid>
                    </Border>
                        <TextBlock  Text="minutes" Grid.Column="1" Name="txt_minute"
                            Foreground="{StaticResource ThickGrey}" FontSize="14"
                                    VerticalAlignment="Center" HorizontalAlignment="Right"
                            Margin="5,5,15,5"/>
                    </Grid>
                    <StackPanel Orientation="Horizontal" Margin="10 5 5 5">
                    
                        <TextBlock    x:Name="txt_activeItem" Grid.Column="3" FontSize="16" 
                                   Margin="5,0,5,0" Text="Active" Foreground="{StaticResource SecondColor}"
                                   HorizontalAlignment="Left" VerticalAlignment="Center"/>
                    <ToggleButton Grid.Column="4" Background="{StaticResource MainColor}" 
                                  IsChecked="{Binding isActive, Mode=OneWay}"
                                  x:Name="tgl_isActiveItem"   Checked="Tgl_isActiveItem_Checked" Unchecked="Tgl_isActiveItem_Unchecked"
                             VerticalAlignment="Center" Padding="10,15,5,5"  
                                          Margin="0" />
                    </StackPanel>
                    <Separator  Background="{StaticResource LightMainColor}" VerticalAlignment="Center" Width="auto" Margin="10 0"  />
                    <StackPanel x:Name="sp_daysWeek" Margin="10" IsEnabled="False">
                        
                    <CheckBox x:Name="chb_all"  Content="All" Margin="5"
                                    Background="{StaticResource MainColor}"
                                  FontWeight="SemiBold" IsChecked="True" 
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>

                            <WrapPanel x:Name="wp_daysWeek" IsEnabled="False" >
                        <CheckBox x:Name="chb_sat"  Content="Saturday " Margin="5"
                                  IsChecked="{Binding sat, Mode=OneWay}" 
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Background="{StaticResource MainColor}"
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>
                        <CheckBox x:Name="chb_sun"  Content="Sunday " Margin="5"
                                  IsChecked="{Binding sun, Mode=OneWay}" 
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Background="{StaticResource MainColor}"
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>
                        <CheckBox x:Name="chb_mon"  Content="Monday" Margin="5"
                                  IsChecked="{Binding mon , Mode=OneWay}" 
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Background="{StaticResource MainColor}" 
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>
                        <CheckBox x:Name="chb_tues"  Content="Tuesday" Margin="5"
                                  IsChecked="{Binding tues , Mode=OneWay}" 
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Background="{StaticResource MainColor}"
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>
                        <CheckBox x:Name="chb_wed"  Content="Wednesday" Margin="5"
                                  IsChecked="{Binding wed , Mode=OneWay}"
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Background="{StaticResource MainColor}" 
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>
                        <CheckBox x:Name="chb_thur"  Content="Thursday" Margin="5"
                                  IsChecked="{Binding thur , Mode=OneWay}"
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Background="{StaticResource MainColor}" 
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>
                        <CheckBox x:Name="chb_fri"  Content="Friday" Margin="5"
                                  IsChecked="{Binding fri , Mode=OneWay}"
                                  Checked="Chb_all_Checked" Unchecked="Chb_all_Unchecked"
                                    Background="{StaticResource MainColor}" 
                                    Style="{StaticResource MaterialDesignFilterChipPrimaryOutlineCheckBox}"/>
                       
                    </WrapPanel>
                    </StackPanel>

                    <!--</Grid>-->
                </StackPanel>
            </ScrollViewer>

            <Border Grid.Row="3" Background="White" CornerRadius="0 0 7 7">
                <Grid  Grid.Row="3" Margin="5,5,5,10" >
                    <Grid.RowDefinitions>
                        <RowDefinition Height="auto"/>
                        <RowDefinition Height="*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="*"/>
                    </Grid.ColumnDefinitions>
                    <Border Background="White" Grid.ColumnSpan="3"  Height="1"  BorderBrush="{StaticResource MainColor}" 
                    BorderThickness="1" Opacity="1" Margin="5,5,5,5"  />
                    <Border Grid.Row="1" Grid.ColumnSpan="3">

                        <Button x:Name="btn_save" Click="Btn_save_Click"   Content="Save" materialDesign:ButtonAssist.CornerRadius="7"
                          Background="{StaticResource SecondColor}" BorderBrush="{x:Null}"
                    Margin="1,1"   Foreground="White" />
                    </Border>
                </Grid>

            </Border>

        </Grid>
        </Border>

    </Grid>
</UserControl>
